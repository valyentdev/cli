{
  "region": "gra",
  "skip_start": false,
  "config": {
    "image": "docker.io/codercom/code-server:latest",
    "workload": {
      "env": ["SUDO_PASSWORD=password", "PASSWORD=password"],
      "restart": {
        "policy": "always"
      },
      "init": {
        "user": "root"
      }
    },
    "guest": {
      "cpu_kind": "eco",
      "cpus": 1,
      "memory_mb": 512
    }
  }
}
